{
  "name": "stock-market-dashboard",
  "version": "1.0.0",
  "description": "React-based stock market dashboard that visualizes a predefined set of stock market indexes from a 3rd party API. This is a project for the React Upskill course from the Telerik Academy.",
  "main": "./src/index.jsx",
  "homepage": "https://jannden.gitlab.io/stock-market-dashboard",
  "repository": "https://gitlab.com/JanNDen/stock-market-dashboard",
  "author": "Jan Cibulka and Teya Andreeva",
  "license": "ISC",
  "scripts": {
    "prepare": "husky install",
    "start": "set NODE_ENV=development&& webpack serve --config ./webpack.config.js --mode development",
    "build": "set NODE_ENV=production&& webpack --config ./webpack.config.js --mode production",
    "husky-test": "echo \"Welcome, this is Husky speaking...\"",
    "lint": "eslint --fix",
    "test": "jest",
    "test:node": "jest --env=node",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:cypress": "start-server-and-test start http://localhost:8080 cypress",
    "cypress": "cypress run",
    "cypress:open": "cypress open"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.4.5",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.5.1",
    "cypress": "^9.3.1",
    "dotenv-webpack": "^7.0.3",
    "enzyme": "^3.11.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.0",
    "jest": "^27.4.5",
    "jest-junit": "^13.0.0",
    "lint-staged": "^12.1.2",
    "prettier": "^2.4.1",
    "react-refresh": "^0.11.0",
    "react-test-renderer": "^17.0.2",
    "redux-mock-store": "^1.5.4",
    "start-server-and-test": "^1.14.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.64.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.3.10",
    "@testing-library/user-event": "^13.5.0",
    "apexcharts": "^3.32.1",
    "bootstrap": "^5.1.3",
    "firebase": "^9.6.1",
    "react": "^17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-bootstrap": "^2.0.1",
    "react-dom": "^17.0.2",
    "react-modal": "^3.14.4",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "save": "^2.4.0"
  }
